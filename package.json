{
    "name": "react4xp-buildconstants",
    "version": "0.0.1",
    "description": "Shared constants for buildtime component libraries for Enonic React4XP",
    "main": "lib/index.js",
    "scripts": {
      "compile": "rimraf lib/* && babel src -d lib",
      "prepublish": "npm run checkall",
      "eslint": "eslint ./*.js src/ test/",
      "checkall": "npm run eslint && npm run compile && npm run test",
      "test": "NODE_PATH=./ mocha --compilers js:babel-core/register --require babel-polyfill --require test/test_helper.js \"test/**/*@(.js)\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enonic/react4xp-buildconstants.git"
    },
    "author": "Espen Norderud",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/enonic/react4xp-buildconstants/issues"
    },
    "homepage": "https://github.com/enonic/react4xp-buildconstants",
    "dependencies": {},
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^7.1.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "6.20.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "mocha": "3.2.0",
        "rimraf": "^2.6.3",
        "chai": "3.5.0",
        "deep-freeze": "^0.0.1",
        "eslint": "3.14.0"
    },
    "babel": {
        "presets": [
            "es2015",
            "react",
            "stage-0"
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    }
}
